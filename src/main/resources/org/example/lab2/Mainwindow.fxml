<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="VboxStyle" fx:id="MainVbox" alignment="CENTER" prefHeight="600.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.lab2.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="100.0" prefWidth="600.0">
      <children>
         <Button id="MainButtons" fx:id="addButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="Добавити">
            <HBox.margin>
               <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
            </HBox.margin>
         </Button>
         <Button id="MainButtons" fx:id="redactionButton" mnemonicParsing="false" onAction="#showRedactionWindow" prefHeight="40.0" prefWidth="140.0" text="Редагувати">
            <HBox.margin>
               <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
            </HBox.margin>
         </Button>
         <Button id="MainButtons" fx:id="deleteButton" mnemonicParsing="false" onAction="#information_Alert" prefHeight="40.0" prefWidth="140.0" text="Видалити">
            <HBox.margin>
               <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
            </HBox.margin>
         </Button>
      </children>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </HBox>
   <AnchorPane maxHeight="100.0" minWidth="-Infinity" prefHeight="100.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
      <children>
         <TextField layoutX="-8.0" layoutY="16.0" prefHeight="40.0" prefWidth="310.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="230.0" />
         <Button id="MainButtons" layoutX="460.0" layoutY="16.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="140.0" text="ПОШУК" AnchorPane.rightAnchor="0.0" />
      </children>
      <VBox.margin>
         <Insets left="30.0" right="30.0" />
      </VBox.margin>
   </AnchorPane>
   <AnchorPane prefHeight="300.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
      <children>
         <TableView fx:id="tableAddressBook" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
           <columns>
             <TableColumn fx:id="columnPIP" prefWidth="299.2000106573105" text="ПІП" />
             <TableColumn fx:id="columnPhone" prefWidth="299.199951171875" text="Телефон" />
           </columns>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         </TableView>
      </children>
      <VBox.margin>
         <Insets left="30.0" right="30.0" />
      </VBox.margin>
   </AnchorPane>
   <AnchorPane prefHeight="100.0" prefWidth="600.0">
      <children>
         <Label id="UserLabel" fx:id="label" text="Кількість користувачів:" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
         <VBox prefHeight="100.0" prefWidth="192.0" AnchorPane.rightAnchor="0.0">
            <children>
               <Button id="MainButtons" fx:id="OtherLabsBtn" maxHeight="40.0" maxWidth="190.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="190.0" text="OTHER LABS" />
               <Button id="MainButtons" fx:id="Exit" maxHeight="40.0" maxWidth="190.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="190.0" text="ВИХІД">
                  <VBox.margin>
                     <Insets top="10.0" />
                  </VBox.margin>
               </Button>
            </children>
         </VBox>
      </children>
      <VBox.margin>
         <Insets left="30.0" right="30.0" />
      </VBox.margin>
   </AnchorPane>
</VBox>
